    <TIER LINGUISTIC_TYPE_REF="translation" PARENT_REF="AYA" TIER_ID="ENG">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a91" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE>‘For example it, what do you call it, the monkey,’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a92" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE>‘um, you know that the monkey is an animal that shouts very loudly.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a93" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE>‘But thunder shouts even more.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a94" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE>‘So then the monkey was shouting up in a tree.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a95" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE>‘Monkey is shouting in a tree.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a96" ANNOTATION_REF="a42">
                <ANNOTATION_VALUE>‘All the time.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a97" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE>‘He left. He went looking for someone who could shout louder.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a98" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE>‘But there is one, they say, there working, who does shout more.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a99" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE>‘Monkey arrived in that place.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a100" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE>‘He said to him, “What are you doing?” ’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a101" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE>‘ “I’m working. And you?” ’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a102" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE>‘ “I’m out walking.” ’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a103" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE>‘ “You have so little work to do?” ’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a104" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE> ‘ “Weeell, I have work to do but I went for a walk anyway.” ’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a105" ANNOTATION_REF="a15">
                <ANNOTATION_VALUE>‘ “Let’s make a bet—let’s see who shouts louder than a… .” ’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a106" ANNOTATION_REF="a16">
                <ANNOTATION_VALUE>‘ “I shout louder than any man!” ’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a107" ANNOTATION_REF="a17">
                <ANNOTATION_VALUE>‘So then, he says to him,’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a108" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE>‘the lightning says, “I don’t shout often,’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a109" ANNOTATION_REF="a19">
                <ANNOTATION_VALUE>‘ “but what the heck?’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a110" ANNOTATION_REF="a20">
                <ANNOTATION_VALUE>‘ “Go climb up on that branch.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a111" ANNOTATION_REF="a21">
                <ANNOTATION_VALUE>‘ “I’m going presently.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a112" ANNOTATION_REF="a22">
                <ANNOTATION_VALUE>‘ “I am going to stop working.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a113" ANNOTATION_REF="a23">
                <ANNOTATION_VALUE>‘ “I’m going to put on my shirt, and I’ll be right there.” ’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a114" ANNOTATION_REF="a24">
                <ANNOTATION_VALUE>‘He went home.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a115" ANNOTATION_REF="a25">
                <ANNOTATION_VALUE>‘He put on his shirt.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a116" ANNOTATION_REF="a26">
                <ANNOTATION_VALUE>‘He dressed himself.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a117" ANNOTATION_REF="a27">
                <ANNOTATION_VALUE>‘He grabbed his sword.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a118" ANNOTATION_REF="a28">
                <ANNOTATION_VALUE>‘Out the door into his patio,’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a119" ANNOTATION_REF="a29">
                <ANNOTATION_VALUE>‘he swung it around.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a120" ANNOTATION_REF="a30">
                <ANNOTATION_VALUE>‘He swung it around and fired a bolt of lightning.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a121" ANNOTATION_REF="a31">
                <ANNOTATION_VALUE>‘It went right for the tree.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a122" ANNOTATION_REF="a32">
                <ANNOTATION_VALUE>‘The bolt split the tree in half.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a123" ANNOTATION_REF="a33">
                <ANNOTATION_VALUE>And from there no one knows where the monkey will end up.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a54" ANNOTATION_REF="a34">
                <ANNOTATION_VALUE>‘A branch hit him and killed him below the tree.’	</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a55" ANNOTATION_REF="a35">
                <ANNOTATION_VALUE>‘Then he says,’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a56" ANNOTATION_REF="a36">
                <ANNOTATION_VALUE>‘Lightning says, “Where is your courage?’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a57" ANNOTATION_REF="a37">
                <ANNOTATION_VALUE>‘ “You say you shout louder?’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a46" ANNOTATION_REF="a38">
                <ANNOTATION_VALUE>‘ “I was going to shout at once, but no.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a47" ANNOTATION_REF="a39">
                <ANNOTATION_VALUE>‘ “Not one little bit.” ’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a48" ANNOTATION_REF="a40">
                <ANNOTATION_VALUE>‘Monkey remained there below the tree.’</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a49" ANNOTATION_REF="a41">
                <ANNOTATION_VALUE>‘That’s what.’ (laughter)</ANNOTATION_VALUE>
