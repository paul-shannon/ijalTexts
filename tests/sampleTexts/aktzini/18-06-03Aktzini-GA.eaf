        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a12" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE>'bueno, te voy a contar este cuento de A'ktzíni'</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a13" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE>'ese A'ktzíni es que bajaba este ya tiene tiempo, este donde esta'</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a14" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE>'esta montagna se llamaba Muchukúku, ahora se le llama Koli:jke' </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a15" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE>'y llegaba hasta al cielo pues bajaba unos animales grandes que daban miedo, se comian las personas' </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a16" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE>'ahora es que le dicen, a ese A'ktzíni:'</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a17" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE>'te queremos pedir un favor, quebra/quita esa montaña' </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a38" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE>'en el mar lo hechas, este, es que le dicen, lo voy a hacer, contesta' </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a39" ANNOTATION_REF="a19">
                <ANNOTATION_VALUE>'ahora es que... ya no estaba esa montaña, es que ya nada mas le pusieron una nube, es que' </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a40" ANNOTATION_REF="a20">
                <ANNOTATION_VALUE>'le dicen vas a ir a impesar desde arriba y asi hasta en el mar lo vas a hechar' </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a41" ANNOTATION_REF="a21">
                <ANNOTATION_VALUE>'ijole, esta A'ktzíni es que fue a empezar hasta arriba'</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a42" ANNOTATION_REF="a22">
                <ANNOTATION_VALUE>'cuando es que estaba pasando se sumerjio, venia pasando hasta llegar al mar' </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a43" ANNOTATION_REF="a23">
                <ANNOTATION_VALUE>'splash, callio porque ya no tenia tierra, ya nada mas era una nube lo que abia allí' </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a44" ANNOTATION_REF="a24">
                <ANNOTATION_VALUE>'y alli alli cayo ahora por eso ahora llora (grita)' </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a45" ANNOTATION_REF="a25">
                <ANNOTATION_VALUE>'ahora nosotros la llamamos A'ktzíni, alli cayo [en el mar]</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a46" ANNOTATION_REF="a26">
                <ANNOTATION_VALUE>'ahora no puede salir de alli porque se quedo en medio del mar' </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
